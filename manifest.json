{
  "manifest_version": 2,
  "name": "Swedish newspaper fixer",
  "description": "This fixes the paywall on dn.se and di.se",
  "version": "2",
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "fixer.js"
      ],
      "matches": [
        "http://www.dn.se/*",
        "https://www.dn.se/*",
        "http://www.di.se/*",
        "https://www.di.se/*",
        "http://weekend.di.se/*",
        "https://weekend.di.se/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
